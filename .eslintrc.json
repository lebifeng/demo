{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint-config-airbnb"],
  "parserOptions": {
    "requireConfigFile": false,
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [
          ["@", "./src"] //别名路径
        ],
        "extensions": [".ts", ".js", ".jsx", ".json"]
      }
    }
  },
  "rules": {
    "react/prop-types": "off",
    "jsx-quotes": "off",
    "no-unused-vars": "warn",
    "import/extensions": "off"
  }
}
